{
	"type": "bigglobe:script",
	"config": {
		"script": [
			"unless (getBlockState(originX, originY - 1, originZ).hasFullCubeOutline(): return(false))",
			"if (`bigglobe:overworld/cave_surface_depth` > 0.0:",
				"random.if (1.0 / 256.0:",
					"return(placeFeature(originX, originY, originZ,",
						"random.nextBoolean()",
						"? 'minecraft:huge_red_mushroom'",
						": 'minecraft:huge_brown_mushroom'",
					"))",
				")",
				"else random.if (1.0 / 64.0:",
					"setBlockState(originX, originY, originZ,",
						"random.nextBoolean()",
						"? 'minecraft:red_mushroom'",
						": 'minecraft:brown_mushroom'",
					")",
					"return(true)",
				")",
				"else random.if (1.0 / 16.0:",
					"setBlockState(originX, originY, originZ, 'bigglobe:mushroom_spores')",
					"return(true)",
				")",
			")",
			"return(false)"
		]
	}
}