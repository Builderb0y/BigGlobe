{
	"type": "bigglobe:overworld_structure_overrider",
	"config": {
		"script": [
			"if (",
				"start.structure == Structure('minecraft:ocean_ruin_cold') ||",
				"start.structure == Structure('minecraft:ocean_ruin_warm')",
			":",
				"return(",
					"(start.structure == Structure('minecraft:ocean_ruin_cold'))",
