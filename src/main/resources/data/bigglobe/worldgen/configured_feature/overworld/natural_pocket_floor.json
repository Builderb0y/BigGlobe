{
	"type": "bigglobe:script",
	"config": {
		"script": [
			"random.if (1.0 / 64.0:",
				"placeFeature(originX, originY, originZ, 'bigglobe:overworld/caves/big_dripleaf')",
			")",
			"else random.if (0.25:",
				"placeBlockState(originX, originY, originZ,",
					"random.if (0.125:",
						"random.nextBoolean(0.25)",
						"? 'minecraft:flowering_azalea'",
						": 'minecraft:azalea'",
					")",
					"else (",
						"random.if (0.25:",
							"random.if (0.25:",
								"random.if (0.25:",
									"BlockState('minecraft:small_dripleaf[facing=north,half=lower,waterlogged=false]')",
									".with('facing', random.switch('north', 'east', 'south', 'west'))",
								")",
								"else (",
									"random.nextBoolean(0.25)",
									"? 'minecraft:large_fern[half=lower]'",
									": 'minecraft:tall_grass[half=lower]'",
								")",
							")",
							"else (",
								"random.nextBoolean(0.25)",
								"? 'minecraft:fern'",
								": 'minecraft:grass'",
							")",
						")",
						"else (",
							"'bigglobe:short_grass'",
						")",
					")",
				")",
			")",
			"return(true)"
		]
	}
}