{
	"type": "bigglobe:script",
	"config": {
		"script": [
			"BlockState originState = getBlockState(originX, originY, originZ)",
			"if (originState.isAir():",
				"if (getBlockState(originX, originY - 1, originZ) == `bigglobe:nether/bubble`.surface_state:",
					"random.if (1.0I / 512.0I:",
						"placeFeature(originX, originY, originZ, 'bigglobe:nether/flowers/valley_of_souls')",
					")",
					"random.if (1.0I / 1024.0I:",
						"placeFeature(originX, originY, originZ, 'bigglobe:nether/flowers/soul_fire')",
					")",
					"random.if (0.5I:",
						"random.if (0.25:",
							"random.if (0.125:",
								"random.if (0.25:",
									"random.if (0.125:",
										"placeBlockState(originX, originY, originZ, BlockState('minecraft:wither_skeleton_skull', rotation: random.nextInt(16)))",
									")",
									"else (",
										"placeBlockState(originX, originY, originZ, BlockState('minecraft:skeleton_skull', rotation: random.nextInt(16)))",
									")",
								")",
								"else (",
									"placeBlockState(originX, originY, originZ, 'minecraft:wither_rose')",
								")",
							")",
							"else (",
								"placeBlockState(originX, originY, originZ, 'bigglobe:wart_weed')",
							")",
						")",
						"else (",
							"placeBlockState(originX, originY, originZ, 'bigglobe:charred_grass')",
						")",
					")",
					"else (",
						"false",
					")",
				")",
				"else if (getBlockState(originX, originY + 1, originZ).isIn('bigglobe:valley_of_souls_base_blocks'):",
					"random.if (",
						"unmixSmooth(",
							"double(`bigglobe:nether/mid_y` - `bigglobe:nether/cavern_fade_radius`),",
							"double(`bigglobe:nether/mid_y` + `bigglobe:nether/cavern_fade_radius`),",
							"double(originY)",
						")",
						"* 0.0625I",
					":",
						"placeFeature(originX, originY, originZ, 'bigglobe:nether/chain')",
					")",
					"else (",
						"false",
					")",
				")",
				"else (",
					"false",
				")",
			")",
			"else (",
				"false",
			")"
		]
	}
}