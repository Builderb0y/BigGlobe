{
	"type": "bigglobe:script",
	"config": {
		"script": [
			"BlockState originState = getBlockState(originX, originY, originZ)",
			"if (originState.isAir():",
				"if (getBlockState(originX, originY - 1, originZ) == `bigglobe:nether/bubble`.surface_state:",
					"random.if (1.0I / 256.0I:",
						"placeFeature(originX, originY, originZ, 'bigglobe:nether/flowers/forest')",
					")",
					"random.if (0.125:",
						"random.if (0.75:",
							"placeFeature(originX, originY, originZ, 'bigglobe:nether/trees/natural/crimson')",
						")",
						"else (",
							"placeBlockState(originX, originY, originZ, 'minecraft:crimson_fungus')",
						")",
					")",
					"else (",
						"random.if (",
							"placeBlockState(originX, originY, originZ, 'minecraft:crimson_roots')",
						")",
						"else (",
							"placeBlockState(originX, originY, originZ, 'bigglobe:wart_weed')",
						")",
					")",
				")",
				"else if (getBlockState(originX, originY + 1, originZ).isIn('minecraft:base_stone_nether'):",
					"random.nextBoolean(1.0I / 64.0I) && placeFeature(originX, originY, originZ, 'bigglobe:nether/glowstone/extra')",
				")",
				"else (",
					"false",
				")",
			")",
			"else if (originState.isIn('minecraft:base_stone_nether'):",
				"double chance = unmixLinear(",
					"double(`bigglobe:nether/max_y`),",
					"double(`bigglobe:nether/min_y`),",
					"double(originY)",
				")",
				"* 0.25L",
				"random.if (chance:",
					"random.if (chance:",
						"random.if (chance:",
							"placeFeature(originX, originY, originZ, 'bigglobe:nether/hidden_lava_large_netherrack')",
						")",
						"else (",
							"placeFeature(originX, originY, originZ, 'bigglobe:nether/hidden_lava_small')",
						")",
					")",
					"else (",
						"setBlockState(originX, originY, originZ, 'minecraft:magma_block')",
						"true",
					")",
				")",
				"else (",
					"false",
				")",
			")",
			"else (",
				"false",
			")"
		]
	}
}