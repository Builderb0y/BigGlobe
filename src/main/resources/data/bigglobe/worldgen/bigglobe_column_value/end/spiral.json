{
	"type": "bigglobe:script",
	"params": {
		"type": "double",
		"is_3d": false
	},
	"script": [
		"double wave = sin((`bigglobe:end/warp_angle` + log2(`bigglobe:end/warp_radius`)) * 8.0L)",
		"double amplitude = `bigglobe:end/warp_radius` * exp2(-4.0L * `bigglobe:end/warp_radius`)",
		"return(wave * amplitude)"
	]
}