{
	"type": "bigglobe:script",
	"params": {
		"type": "float",
		"is_3d": false
	},
	"script": [
		"float unmixed = unmixLinear(`bigglobe:end/ring_cloud_min_radius`, `bigglobe:end/ring_cloud_max_radius`, `bigglobe:end/warp_radius`.as(float))",
		"return(unmixed * (1.0I - unmixed) * 4.0I - 1.0I)"
	]
}