{
	"type": "bigglobe:script",
	"params": {
		"type": "float",
		"is_3d": true
	},
	"script": [
		"double relativeToMountains = abs(y - `bigglobe:end/mountain_center`)",
		"double relativeToCloudCenter = relativeToMountains - `bigglobe:end/bridge_cloud_center_offset`",
		"double scaled = relativeToCloudCenter / `bigglobe:end/bridge_cloud_thickness`",
		"return(-float(scaled ^ 2))"
	],
	"cache": false
}