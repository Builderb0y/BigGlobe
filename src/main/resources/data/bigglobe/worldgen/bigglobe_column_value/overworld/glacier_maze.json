{
	"type": "bigglobe:script",
	"params": {
		"type": "float",
		"is_3d": false
	},
	"script": [
		"float maze = `bigglobe:overworld/raw_glacier_maze_noise`",
		"maze = 24.0I - 24.0I / (maze ^ 4 + 1.0I)",
		"float magicalness = 1.0I - 0.0625I / (`bigglobe:overworld/magicalness` ^ 2 + 0.0625I)",
		"float innerness = 1.0I - 0.25I / ((`bigglobe:overworld/glacier_crack_threshold` - 1.0I) ^ 2 + 0.25I)",
		"return(maze * magicalness * innerness)"
	],
	"valid": {
		"where": "`bigglobe:overworld/glacier_crack_threshold` > 1.0L && `bigglobe:overworld/magicalness` > 0.0L",
		"fallback": 0.0
	}
}