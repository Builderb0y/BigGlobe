{
	"type": "bigglobe:script",
	"params": {
		"type": "double",
		"is_3d": false
	},
	"script": [
		"double snowHeight = `bigglobe:overworld/cliffy_snow`",
		"snowHeight + (-2.0L + snowHeight * 0.015625L + `bigglobe:overworld/raw_temperature` * -4.0L)"
	],
	"valid": {
		"where": "`bigglobe:overworld/surface_y` > 0.0L"
	}
}