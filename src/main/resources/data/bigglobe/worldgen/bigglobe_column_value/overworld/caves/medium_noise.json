{
	"type": "bigglobe:script",
	"params": {
		"type": "float",
		"is_3d": true
	},
	"script": [
		"float curve(float valueSquared, float width:",
			"return(valueSquared / (valueSquared + width ^ 2))",
		")",
		"return(",
			"curve(",
				"`bigglobe:overworld/caves/biased_primary_noise`(y),",
				"`bigglobe:overworld/caves/medium_width`(y)",
			")",
			"* curve(",
				"`bigglobe:overworld/caves/biased_secondary_noise`(y),",
				"8.0I",
			")",
		")"
	],
	"valid": {
		"min_y": "ceilInt(`bigglobe:overworld/surface_y`) - `bigglobe:overworld/caves/medium_depth`",
		"max_y": "ceilInt(`bigglobe:overworld/surface_y`)"
	}
}