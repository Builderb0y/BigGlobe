{
	"type": "bigglobe:script",
	"params": {
		"type": "double",
		"is_3d": false
	},
	"script": [
		"double threshold = max(`bigglobe:overworld/surface_temperature` - 0.75L, 0.0L)",
		"threshold *= max(`bigglobe:overworld/magicalness`, 0.0L)",
		"threshold *= unmixSmooth(0.0L, 8.0L, `bigglobe:overworld/basic_surface_y`)",
		"return(threshold / (threshold + 1.0L))"
	]
}