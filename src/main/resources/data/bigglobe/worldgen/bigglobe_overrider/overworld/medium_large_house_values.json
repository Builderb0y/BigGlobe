{
	"type": "column_value",
	"script": [
		"for (StructureStart start in structures:",
			"if (",
				"start.structure == Structure('bigglobe:medium_house') ||",
				"start.structure == Structure('bigglobe:large_house') ||",
				"start.structure == Structure('bigglobe:log_cabin')",
			":",
				"double distance = distanceToSquare(start)",
				"double mixLevel = smooth(distance * 0.0625L)",
				"int minY = start.minY",
				"if (start.structure == Structure('bigglobe:log_cabin'): ++minY)",
				"`bigglobe:overworld/processed_surface_y` = mixLinear(minY - 0.5L, `bigglobe:overworld/processed_surface_y`, mixLevel)",
				"`bigglobe:overworld/basic_surface_y`     = mixLinear(minY - 0.5L, `bigglobe:overworld/basic_surface_y`,     mixLevel)",
				"`bigglobe:overworld/snow_y`              = mixLinear(minY - 1.5L, `bigglobe:overworld/snow_y`,              mixLevel)",
				"`bigglobe:overworld/surface_foliage`    *= float(mixLevel)",
			")",
		")"
	]
}