{
	"type": "structure",
	"script": [
		"if (start.structure.type == StructureType('minecraft:ocean_monument'):",
			"unless (`bigglobe:overworld/hilliness`(start.midX, start.midZ) < 0.375:",
				"return(false)",
			")",
			"int fromY = start.minY",
			"int toY = max(",
				"int(`bigglobe:overworld/processed_surface_y`(start.minX, start.minZ)),",
				"int(`bigglobe:overworld/processed_surface_y`(start.minX, start.maxZ)),",
				"int(`bigglobe:overworld/processed_surface_y`(start.maxX, start.minZ)),",
				"int(`bigglobe:overworld/processed_surface_y`(start.maxX, start.maxZ))",
			")",
			"start.move(toY - fromY)",
			"if (start.maxY >= `bigglobe:overworld/sea_level`(start.midX, start.midZ):",
				"return(false)",
			")",
		")",
		"return(true)"
	]
}