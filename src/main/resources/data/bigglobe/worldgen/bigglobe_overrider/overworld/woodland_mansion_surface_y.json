{
	"type": "column_value",
	"script": [
		"for (StructureStart start in structures:",
			"if (start.structure.type == StructureType('minecraft:woodland_mansion'):",
				"double distance = distanceToSquare(start)",
				"double mixLevel = smooth(1.0 - distance * 0.0625)",
				"`bigglobe:overworld/basic_surface_y` = mixLinear(`bigglobe:overworld/basic_surface_y`, start.minY - 0.5L, mixLevel)",
				"double oldY = `bigglobe:overworld/processed_surface_y` =: mixLinear(`bigglobe:overworld/processed_surface_y`, start.minY - 0.5L, mixLevel)",
				"`bigglobe:overworld/snow_y` += `bigglobe:overworld/processed_surface_y` - oldY",
			")",
		")"
	]
}