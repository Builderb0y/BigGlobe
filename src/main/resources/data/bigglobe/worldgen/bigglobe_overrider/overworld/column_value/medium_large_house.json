{
	"type": "column_value",
	"script": [
		"for (StructureStart start in structures:",
			"if (",
				"start.structure ==. 'bigglobe:medium_house' ||",
				"start.structure ==. 'bigglobe:large_house' ||",
				"start.structure ==. 'bigglobe:log_cabin'",
			":",
				"double distance = distanceToSquare(start)",
				"double mixLevel = smooth(distance * 0.0625L)",
				"int minY = start.minY",
				"if (start.structure ==. 'bigglobe:log_cabin': ++minY)",
				"world_traits.`bigglobe:exact_surface_y` = mixLinear(minY - 0.5L, world_traits.`bigglobe:exact_surface_y`, mixLevel)",
				"world_traits.`bigglobe:approximate_surface_y`     = mixLinear(minY - 0.5L, world_traits.`bigglobe:approximate_surface_y`,     mixLevel)",
				"world_traits.`bigglobe:snow_y`              = mixLinear(minY - 1.5L, world_traits.`bigglobe:snow_y`,              mixLevel)",
				"`bigglobe:overworld/surface_foliage`    *= float(mixLevel)",
			")",
		")"
	]
}