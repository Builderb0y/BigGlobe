{
	"type": "column_value",
	"script": [
		"for (StructureStart start in structures:",
			"if (start.structure ==. 'bigglobe:small_house':",
				"StructurePiece piece = start.pieces.(0)",
				"double distance = distanceToCircle(piece, 4.0L)",
				"double mixLevel = smooth(distance * 0.0625L)",
				"world_traits.`bigglobe:exact_surface_y` = mixLinear(piece.minY - 0.5L, world_traits.`bigglobe:exact_surface_y`, mixLevel)",
				"world_traits.`bigglobe:approximate_surface_y`     = mixLinear(piece.minY - 0.5L, world_traits.`bigglobe:approximate_surface_y`,     mixLevel)",
				"world_traits.`bigglobe:snow_y`              = mixLinear(piece.minY - 1.5L, world_traits.`bigglobe:snow_y`,              mixLevel)",
				"`bigglobe:overworld/surface_foliage`    *= float(mixLevel)",
				"if (start.pieces.size > 1:",
					"piece = start.pieces.(1)",
					"distance = distanceToSquare(piece)",
					"mixLevel = smooth(distance * 0.0625L)",
					"world_traits.`bigglobe:exact_surface_y` = mixLinear(piece.minY + 0.5L, world_traits.`bigglobe:exact_surface_y`, mixLevel)",
					"world_traits.`bigglobe:approximate_surface_y`     = mixLinear(piece.minY + 0.5L, world_traits.`bigglobe:approximate_surface_y`,     mixLevel)",
					"world_traits.`bigglobe:snow_y`              = mixLinear(piece.minY - 0.5L, world_traits.`bigglobe:snow_y`,              mixLevel)",
					"`bigglobe:overworld/surface_foliage`    *= float(mixLevel)",
				")",
			")",
		")"
	]
}