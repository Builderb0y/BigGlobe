{
	"type": "column_value",
	"script": [
		"for (StructureStart start in structures:",
			"if (start.structure.type ==. 'minecraft:woodland_mansion':",
				"double distance = distanceToSquare(start)",
				"double mixLevel = smooth(1.0 - distance * 0.0625)",
				"double oldY = `bigglobe:islands/eroded_surface_height` =: mixLinear(`bigglobe:islands/eroded_surface_height`, start.minY + 0.5L, mixLevel)",
				"`bigglobe:islands/snow_height` += `bigglobe:islands/eroded_surface_height` - oldY",
			")",
		")"
	]
}